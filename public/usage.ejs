<!DOCTYPE html>

<html>
	<head>
	    <% include head %>
	</head>
	<body>
		<div class="container-fluid">
			<% include navbar %>
			<div id="pieChart"></div>
			<script src="https://cdnjs.cloudflare.com/ajax/libs/d3/3.4.4/d3.min.js"></script>
			<script src="assets/js/d3pie.js"></script>
			<script src="assets/js/piechart.js"></script>
			<script type="text/javascript">
				// Tests the math functions to make sure they work properly 
				describe("toBe", function() { 
		            it("should pass equal numbers", function() { 
		              expect(1).toBe(1);  });

		            it("should pass equal strings", function() {
		              expect("testing").toBe("testing"); 
		            });

		            it("should pass equal booleans", function() {expect(true).toBe(true);});

		            it("should pass same objects", function() {var object = {a: "testing"};
		              expect(object).toBe(object); 
		            });

		            it("should pass same arrays", function() {var array = [1, 2, 3];
		              expect(array).toBe(array); 
		            });

		            it("should not pass equal objects", function() {expect({a: "testing"}).not.toBe({a: "testing"});}); 
		             
		            it("should not pass equal arrays", function() { 
		              expect([1, 2, 3]).not.toBe([1, 2, 3]);  }); 
	        	});

	        	//Tests D3 
	            describe("toBe", function(){
		            it("should have 40% slack usage", function(){
		            	var object = {text: "p0_segmentPercentage0-inner"}
		            	expect(object).toBe("40%");
		            });

		            it("should have 33% spotify usage", function(){
		            	var object = {text: "p0_segmentPercentage1-inner"}
		            	expect(object).toBe("33%");
		            });

		            it("should have 15% facebook usage", function(){
		            	var object = {text: "p0_segmentPercentage2-inner"}
		            	expect(object).toBe("15%");
		            });

		            it("should have 12% twitter usage", function(){
		            	var object = {text: "p0_segmentPercentage3-inner"}
		            	expect(object).toBe("12%");
		            });
		        });
			</script>
		</div>
		<% include footer %>
	</body>
</html>
